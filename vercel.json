{
    "version": 2,
    "builds": [
      {
        "src": "src/api/index.ts",
        "use": "@vercel/node",
        "config": {
          "typescript": {
            "tsconfig": "./tsconfig.json"
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/api/index.ts"
      }
    ]
  }
  